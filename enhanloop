public class EnhancedLoopExample {
    public static void main(String[] args) {
        int[] numbers = { 10, 20, 30, 40, 50 };
        int sum = 0;

        // Enhanced for loop to calculate the sum
        for (int number : numbers) {
            sum += number;
        }

        System.out.println("Sum of numbers: " + sum);
    }
}
